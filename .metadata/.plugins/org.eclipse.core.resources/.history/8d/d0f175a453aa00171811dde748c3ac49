		$(".group10").attr("checked", false);
		$(".group11").attr("checked", false);
		$(".group12").attr("checked", true);			
		$(".feeYnView").hide();
		$("#userGubun2Select").removeClass("hdata_left");
		$("#userGubun2Select").addClass("hdata_left2");		
		$("#newCustomerView").hide();
		$("#newCustomerView_line").hide();
	}
}

/* 
 * 1번째 Tab 검색버튼 클릭시 호출 함수
 */
function search04() {
	// 사무소공장
	if($("input:checkbox[name=factoryYn]").is(":checked")) {
		$("input:checkbox[name=factoryYn]").val("Y");
	}else{
		$("input:checkbox[name=factoryYn]").val("N");
	}
	
	
	loading(true, LOADING_TEXT_01);
		
	document.searchform.mode.value = "searchCustomer";
    document.searchform.customerSearchOptionKind.value = CUSTOMER_SEARCH_OPTION_KIND_IS_BUSINESSINFO;
	document.searchform.isResult.value = "N";
	document.searchform.currentPage.value = 1;
	//document.searchform.target = "xframe";
	//document.searchform.action ="/kita/cstmrinfomanage/cstmrinqire/corpotationcustomersearchBassinfo.do";
	
	//document.searchform.submit();
	makeResultList();
}

function makeResultList(){
    commonUtil.ajaxCall('<c:url value="/kita/cstmrinfomanage/cstmrinqire/corpotationcustomersearchBassinfoForAjax.do" />', $("form#searchform").serialize(), function(result){
			var jsondata = result.resultList;
			var img_url="";
			var boardOrgNm = "";
			var step=0;
			var buffer="";
			buffer+="<table class='tb_list'>";
			buffer+="    <tr align='center' class='_tablesorter'>";
			buffer+="        <th width='5%' class='head_center1'>";
			buffer+="            선택";
			buffer+="        </th>";  
			buffer+="        <th width='8%' data-column-name='USER_FG' class='head_center1' data-sort-asc-func='sortFildSearch01(\"USER_FG\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"USER_FG\",\"DESC\")'><div>법인구분3</div></th>";
			buffer+="        <th width='9%' data-column-name='MEMBER_ID' class='head_center1' data-sort-asc-func='sortFildSearch01(\"MEMBER_ID\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"MEMBER_ID\",\"DESC\")'><div>무역업번호</div></th>";				
			buffer+="        <th width='9%' data-column-name='COMP_NO' class='head_center1' data-sort-asc-func='sortFildSearch01(\"COMP_NO\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"COMP_NO\",\"DESC\")'><div>사업자번호</div></th>";				
			buffer+="        <th width='18%' data-column-name='CORP_KOR_NM' class='head_center1' data-sort-asc-func='sortFildSearch01(\"CORP_KOR_NM\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"CORP_KOR_NM\",\"DESC\")'><div>회사명</div></th>";				
			buffer+="        <th width='8%' data-column-name='CEO_KOR_NM' class='head_center1' data-sort-asc-func='sortFildSearch01(\"CEO_KOR_NM\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"CEO_KOR_NM\",\"DESC\")'><div>대표자명</div></th>";				
			buffer+="        <th width='8%' data-column-name='MAIN_TEL_NO' class='head_center1' data-sort-asc-func='sortFildSearch01(\"MAIN_TEL_NO\",\"ASC\")' data-sort-desc-func=\"sortFildSearch01(\"MAIN_TEL_NO\",\"DESC\")'><div>전화번호</div></th>";				
			buffer+="        <th width='13%' data-column-name='MAIN_EMAIL' class='head_center1' data-sort-asc-func='sortFildSearch01(\"MAIN_EMAIL\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"MAIN_EMAIL\",\"DESC\")'><div>대표이메일</div></th>";				
			buffer+="        <th width='7%' data-column-name='INTEREST_YN' class='head_center3' data-sort-asc-func='sortFildSearch01(\"INTEREST_YN\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"INTEREST_YN\",\"DESC\")'><div>관심고객</div></th>";				
			buffer+="        <th width='7%' data-column-name='FEE_CNT' class='head_center3' data-sort-asc-func='sortFildSearch01(\"FEE_CNT\",\"ASC\")' data-sort-desc-func='sortFildSearch01(\"FEE_CNT\",\"DESC\")'><div>회비납부</div></th>";				
			buffer+="    </tr>";
			if (result.resultList.length == 0) buffer+="    <tr><td class='alC' colspan='10'>검색된 결과가 없습니다.</td></tr>";
			for (var i=0; i < jsondata.length; i++){
			    if(_cmmutils_json_null(jsondata[i].userFg)!='')
					img_url = "<img class='zero' src='/images/kita_m0" + _cmmutils_json_null(jsondata[i].userFg) + ".gif' width='70' height='23' data-corp-seq='"+_cmmutils_json_null(jsondata[i].corpSeq)+"'>";
				if(_cmmutils_json_null(jsondata[i].boardOrgNm)!='')
				    boardOrgNm = _cmmutils_json_null(jsondata[i].boardOrgNm);
				if(_cmmutils_json_null(jsondata[i].blngCd)!='01')
								buffer+="    <tr style='background-color: #ffe7e7;'>";
				else
								buffer+="    <tr>";
			    buffer+="        <td class='alC'>";
			    buffer+="            <input type='checkbox' name='targetCustomer' class='input_basic_w' onclick='javascript:checkTargetCustomer(this);' value='" + _cmmutils_json_null(jsondata[i].corpSeq) + "'>";
			    buffer+="        </td>";
			    buffer+="        <td class='alC'>" + img_url + "</td>";
			    buffer+="        <td class='alC'>" + _cmmutils_json_null(jsondata[i].memberId) + "</td>";
			    buffer+="        <td class='alC'>" + _cmmutils_json_null(jsondata[i].compNo) + "</td>";
			    buffer+="        <td>";
				buffer+="            <a href=\"javascript:goDetail('" + _cmmutils_json_null(jsondata[i].corpSeq) + "', '" + _cmmutils_json_null(jsondata[i].memberId) + "');\">";
	        	buffer+=                 _cmmutils_json_null(jsondata[i].corpKorNm);
	        	buffer+="            </a>";
	        								   if(_cmmutils_json_null(jsondata[i].corpStateCd)=="01")
	        	buffer+="            	   		<img class=\"vaM mgl5\" src=\"/imgNew/ico_hue.gif\"> ";
	        								   else if(_cmmutils_json_null(jsondata[i].corpStateCd)=="02")
	        	buffer+="            	   		<img class=\"vaM mgl5\" src=\"/imgNew/ico_pye.gif\"> ";
			    buffer+="        </td>";
			    buffer+="        <td class='alC'>" + _cmmutils_json_null(jsondata[i].ceoKorNm) + "</td>";
	            buffer+="        <td class='alC'>" + _cmmutils_json_null(jsondata[i].mainTelNo) + "</td>";
	            buffer+="        <td class='alC'>" + _cmmutils_json_null(jsondata[i].mainEmail)  + "</td>";
	            buffer+="        <td id='interestYn" + (step++) + "' class='alC'>" + _cmmutils_json_null(jsondata[i].interestYn) + "</td>";
	            if(_cmmutils_json_null(jsondata[i].memberGbn)=="1")
	                if(_cmmutils_json_null(jsondata[i].feeCnt)=="")
		                buffer+="        <td class='alC'>완납</td>";
	                else
		                buffer+="        <td class='alC'>" + _cmmutils_json_null(jsondata[i].feeCnt) + "년미납</td>";
				else
		        	buffer+="        <td class='alC'>&nbsp;</td>";
			    buffer+="    </tr>";
			}
			buffer+="</table>";
			loading(false);
			$('#customerSearchOptionKindText').text(result.reqDataVO.searchOptionKindText);
			searchform.totalCount.value = result.reqDataVO.totalCount;
			$('#customerCount').text(putComma(result.reqDataVO.totalCount));
			$('#customerList').html(buffer);
			$('#listPage').html(result.paginationInfo);
			$("input[data-name='sql']").val(result.reqDataVO.sql);
			setCheck();
			initTableSorter();
			fnAddInfo();
	}, 'put', false);
}
/* 
 * 2번째 Tab 검색버튼 클릭시 호출 함수
 */
function search01() {
	//지부관할 값 셋팅	
	if( $("input[name=selectJibu]").is(":checked") ){
		$("input[name=selectJibu]").val("Y");
	}else{
		$("input[name=selectJibu]").val("");
	}
	
	if(document.searchform.yyyy.value == "") {
		alert("기간을 입력해 주세요.");
		document.searchform.yyyy.focus();
		return;
	}
	if(document.searchform.mm.value == "") {
		alert("기간을 입력해 주세요.");
		document.searchform.mm.focus();
		return;
	}
	
	document.searchform.mode.value = "searchCustomer";
    document.searchform.customerSearchOptionKind.value = CUSTOMER_SEARCH_OPTION_KIND_IS_TRADEINFO;
	document.searchform.isResult.value = "N";
	document.searchform.currentPage.value = 1;
	//document.searchform.target = "xframe";
	//document.searchform.action ="/jsp/customer/KCustBasicInfoSearch_company.jsp";	
	//document.searchform.submit();
	
	document.searchform.condRelCd.value = document.searchform.cond_rel_cd.value;
	document.searchform.condRelNm.value = document.searchform.cond_rel_nm.value;
	document.searchform.condJeGb.value = document.searchform.cond_je_gb.value;
	document.searchform.condCtrCd.value = document.searchform.cond_ctr_cd.value;
	document.searchform.condCtrNm.value = document.searchform.cond_ctr_nm.value;
	document.searchform.condPrdtbrGb.value = document.searchform.cond_prdtbr_gb.value;
	document.searchform.condPrdtCd.value = document.searchform.cond_prdt_cd.value;
	document.searchform.condPrdtNm.value = document.searchform.cond_prdt_nm.value;
	document.searchform.qtyUnit.value = document.searchform.qty_unit.value;
	
	makeResultList();
}

