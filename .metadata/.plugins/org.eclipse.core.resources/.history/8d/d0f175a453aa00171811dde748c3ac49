		document.searchform.submit();
	}



/**
 * 품목정보를 선택시의 이벤트를 처리한다.
 */
function clickItemFg(name, radioNm) {

    var valObj   = document.getElementById(name);
    var radioObj = document.getElementsByName(radioNm);

    for(var i=0; i<radioObj.length; i++) {
	    if(radioObj[i].checked == true) {
		    valObj.value = radioObj[i].value;
	    }
    }

    if(radioNm == "itemFg") {
        document.searchform.cond_prdt_cd.value = "";
        document.searchform.cond_prdt_nm.value = "";
    }
}

/**
 * 조회조건에 해당하는 조건선택팝업을 호출한다.
 */
function openPopup(url) {

    var url;

    /*
    if(url.indexOf("gikt9550d1.jsp") != -1) { //권역별
        document.searchform.condCtrCd.value = "";
        document.searchform.condCtrNm.value = "";
    }
    */
    if(url.indexOf("searchContPop.screen") != -1) { //권역별
        document.searchform.cond_ctr_cd.value = "";
        document.searchform.cond_ctr_nm.value = "";
    }


    /*
    if(url.indexOf("gikt9540d.jsp") != -1) { //국가별
        document.searchform.condRelCd.value = "";
        document.searchform.condRelNm.value = "";
    }
    */
    if(url.indexOf("searchCountryPop.screen") != -1) { //국가별
        document.searchform.cond_rel_cd.value = "";
        document.searchform.cond_rel_nm.value = "";
    }

    //권역별
    var condRelCd = document.searchform.cond_rel_cd.value;
    var condRelNm = document.searchform.cond_rel_nm.value;

    //국가별
    var condCtrCd = document.searchform.cond_ctr_cd.value;
    var condCtrNm = document.searchform.cond_ctr_nm.value;

    //품목별
    var condPrdtbrGb = document.searchform.cond_prdtbr_gb.value; //품목분류
    var cond_ie_gb     = document.searchform.ieGbn.value ; //수출입구분

    if(condPrdtbrGb == "") {
        condPrdtbrGb = "1";
    }
    
    if(cond_ie_gb == "" || cond_ie_gb == "E") {
        cond_ie_gb = "1";
    } else {
        cond_ie_gb = "2";
    }

    /*
    if(url.indexOf("gikt9510d.jsp") != -1) {
        url += "&condPrdtbrGb=" + condPrdtbrGb;
        if(condPrdtbrGb == "2") {
        	url += "&cond_ie_gb=" + cond_ie_gb;
        }
    }
    */
    if(url.indexOf("searchItemPop.screen") != -1) {
        url += "&condPrdtbrGb=" + condPrdtbrGb;
        /*
        if(condPrdtbrGb == "2") {
        	url += "&cond_ie_gb=" + cond_ie_gb;
        }
        */
    }

    var New_Win = window.open(url, "New_Win", "toolbar=no, width=800, height=710, location=no, status=yes, scrollbars=yes, directories=no, menubar=no, resizable=yes");

    New_Win.focus();
}

/**
 * 업체소재지 전체를 선택한다.
 */
function clickCheckAll(allChkNm, selChkNm) {
	
    var allObj = document.getElementById(allChkNm);
    var selObj = document.getElementsByName(selChkNm);

    if(allObj.checked == true) {
        for(var i=0; i<selObj.length; i++) {
            selObj[i].checked = true;
        }
    } else {
        for(var i=0; i<selObj.length; i++) {
            selObj[i].checked = false;
        }
    }
}

/**
 * 사업목록팝업을 호출한다.
 */
function openCompNamePopup() {
	//window.open('/jsp/BM/BM001020ServicePopup.jsp','BM001020ServicePopup_jsp', 'top=10,left=10,width=100,height=100,toolbar=no, status=yes, menubar=no, scrollbars=yes');
	window.open('/jsp/customer/KCustServiceSearchPopup.jsp', "New_Win", "toolbar=no, width=800, height=710, location=no, status=yes, scrollbars=yes, directories=no, menubar=no, resizable=yes");
}

/**
 * 목록화면을 인쇄한다.
 */
function pagePrint() {
	
	if(parseInt($("#customerCount").text()) == 0) {	
		alert("고객 조회 후 인쇄 가능합니다.");
		return;
	}
	document.searchform.target = "_blank";
	document.searchform.action = "/jsp/customer/KCustBasicInfoSearch_company_print.jsp";
	document.searchform.submit();	
 }
/* 
 * 사업명 팝업 callback function
 */
function callbackServicePopup(pID, gID, sID, serviceNm) {
	document.searchform.serviceNm.value = serviceNm;
	document.searchform.servicePID.value = pID;
	$("#servicePID").change();
	document.searchform.serviceGID.value = gID;
	document.searchform.serviceSID.value = sID;
}

/**
 * 
 */

function showOtherOption2(){
	if($("input:radio[name='newCustomer']:checked").val() == "1"){
		if($("select[name='userGubun2']").val() == "01"){
			$("#newCustomerView1").show();
			$("#newCustomerView_line1").show();
		}else{
			$("#newCustomerView1").hide();
			$("#newCustomerView_line1").hide();
		}
	}else{
		$("#newCustomerView1").hide();
		$("#newCustomerView_line1").hide();
	}
}

//회원구분 콤보박스 체인지 이벤트 (기본 정보)
function userGubunChange1(){
	if($("._userGubun1").val() == "all"){
		$("#01").hide();	
		$("#02").hide();	
		$("#03").hide();	
		$("#04").hide();	
		$("#05").hide();	
		$("#06").hide();	
		$("input[name='userGubunBox1']").attr("checked", false);
	}else if($("._userGubun1").val() == "01"){
		$("#01").show();	
		$("#02").hide();	
		$("#03").hide();	
		$("#04").hide();	
		$("#05").hide();	
		$("#06").hide();	
		$(".group01").attr("checked", true);
		$(".group02").attr("checked", false);
		$(".group03").attr("checked", false);
		$(".group04").attr("checked", false);
		$(".group05").attr("checked", false);
	}else if($("._userGubun1").val() == "02"){
		$("#01").hide();	
		$("#02").show();	
		$("#03").hide();	
		$("#04").hide();	
		$("#05").hide();	
		$("#06").hide();	
		$(".group01").attr("checked", false);
		$(".group02").attr("checked", true);
		$(".group03").attr("checked", false);
		$(".group04").attr("checked", false);
		$(".group05").attr("checked", false);
	}else if($("._userGubun1").val() == "03"){
		$("#01").hide();	
		$("#02").hide();	
		$("#03").show();	
		$("#04").hide();	
		$("#05").hide();	
		$("#06").hide();
		$(".group01").attr("checked", false);
		$(".group02").attr("checked", false);
		$(".group03").attr("checked", true);
		$(".group04").attr("checked", false);
		$(".group05").attr("checked", false);
	}else if($("._userGubun1").val() == "04"){
		$("#01").hide();	
		$("#02").hide();	
		$("#03").hide();	
		$("#04").show();	
		$("#05").hide();	
		$("#06").hide();	
		$(".group01").attr("checked", false);
		$(".group02").attr("checked", false);
		$(".group03").attr("checked", false);
		$(".group04").attr("checked", true);
		$(".group05").attr("checked", false);
	}else if($("._userGubun1").val() == "05"){
		$("#01").hide();	
		$("#02").hide();	
		$("#03").hide();	
		$("#04").hide();	
		$("#05").show();	
		$("#06").hide();	
		$(".group01").attr("checked", false);
		$(".group02").attr("checked", false);
		$(".group03").attr("checked", false);
		$(".group04").attr("checked", false);
		$(".group05").attr("checked", true);
	}
}

//회원구분 콤보박스 체인지 이벤트 (회원특성별)
