	alert("");	
}

function fnPrint(){
	//window.open("/inc/common_print.jsp", "print", "status=yes,scrollbars=yes,resizable=yes,width=1200,height=1000,left=50,top=50");
	window.open("/kita/inc/commonPrint.do", "print", "status=yes,scrollbars=yes,resizable=yes,width=1200,height=1000,left=50,top=50");
}

function fnExcelDown(){
	document.excelDownloadForm.html.value = $("div.PrintArea").html(); 
	document.excelDownloadForm.submit();
}

function fnExcelUpload() { 
	var fileDir = $("input:file[name=excel]").val();
	if(fileDir.length > 0){
		loading(true, LOADING_TEXT_01);
		
		var type = fileDir.substring(fileDir.lastIndexOf(".") + 1).toLowerCase();
		if("xls" == type){
		    $('form[name="searchform"]').attr("enctype", "multipart/form-data");
		    commonUtil.ajaxCall('<c:url value="/kita/cstmrinfomanage/cstmrinqire/corpotationcustomersearchBassinfoExcelList.do" />', $("form#searchform").serialize(), function(result){
					var jsondata = result.resultList;
					var buffer="";
					buffer+='<div id="customerList" class="tBox">';
					buffer+='<table id="pageTable" class="tb_list tablesorter">';
					buffer+='<thead>';
					buffer+='<tr>';
					buffer+='<th><div>고객구분</div></th>';
					buffer+='<th><div>무역업번호</div></th>';
					buffer+='<th><div>사업자번호</div></th>';
					buffer+='<th><div>회사명</div></th>';
					buffer+='<th><div>대표자명</div></th>';
					buffer+='<th><div>개인구분</div></th>';
					buffer+='<th><div>담당자</div></th>';
					buffer+='<th><div>전화번호</div></th>';
					buffer+='<th><div>담당자이메일</div></th>';
					buffer+='</tr>';
					buffer+='</thead>';
					buffer+='<tbody class="_tbody_sort">';
					for (var i=0; i < jsondata.length; i++){
						buffer+='<tr>';
						buffer+='<td class="alC">';
						if(_cmmutils_json_null(jsondata[i].corpUserFg)!='')
							buffer+='<img src="/images/kita_m0'+_cmmutils_json_null(jsondata[i].corpUserFg)+'.gif" width="70" height="23">';
						buffer+='</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].memberId)+'</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].compNo)+'</td>';
						buffer+='<td>';
						if(_cmmutils_json_null(jsondata[i].corpSeq)!='')
							buffer+='<a href="javascript:fnCorpDetail(\"'+_cmmutils_json_null(jsondata[i].corpSeq)+','+ _cmmutils_json_null(jsondata[i].memberId)+'\")">';
						buffer+=_cmmutils_json_null(jsondata[i].compKorNm;
						if(_cmmutils_json_null(jsondata[i].corpStateCd)!='01')
							buffer+='<img class="vaM mgl5" src="/imgNew/ico_hue.gif" style="border:0px;">';
						else if(_cmmutils_json_null(jsondata[i].corpStateCd)!='02')
							buffer+='<img class="vaM mgl5" src="/imgNew/ico_pye.gif" style="border:0px;">';
						if(_cmmutils_json_null(jsondata[i].corpSeq)!='')
							buffer+='</a>';
						buffer+='</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].compKorCeo)+'</td>';
						buffer+='<td class="alC">';
						if(_cmmutils_json_null(jsondata[i].userFg)!='')
							buffer+='<img src="/images/kita_m0'+_cmmutils_json_null(jsondata[i].userFg)+'.gif" width="70" height="23">';
						buffer+='</td>';
						buffer+='<td class="alC">';
						if(_cmmutils_json_null(jsondata[i].seq)!='')
							buffer+='<a href="javascript:fnPersonDetail(\"'+_cmmutils_json_null(jsondata[i].seq)+','+ _cmmutils_json_null(jsondata[i].memberId)+'\")">';
						buffer+=_cmmutils_json_null(jsondata[i].contactNm);
						if(_cmmutils_json_null(jsondata[i].seq)!='')
							buffer+='</a>';
						buffer+='</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].contactTel)+'</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].contactEmail)+'</td>';
						buffer+='</tr>';
					}
					buffer+='</tbody>';
					buffer+='</table>';
					buffer+='</div>';
					buffer+='<div class="sp10"></div>';
					buffer+='<div id="pager"  class="pager paging" align="center">';
					buffer+='<img src="/imgNew/ico_list_first.png" class="vaM first imgBtn">';
					buffer+='<img src="/imgNew/ico_list_prev.png" class="vaM prev imgBtn">';
					buffer+='<span class="pagedisplay"></span>';
					buffer+='<img src="/imgNew/ico_list_next.png" class="vaM next imgBtn">';
					buffer+='<img src="/imgNew/ico_list_last.png" class="vaM last imgBtn">';
					buffer+='</div>';
					buffer+='<div class="btnBox fr">';
					buffer+='<div class="btFbox w70" onclick="fnPrint()"><div class="btF">인쇄하기</div></div>';
					buffer+='<div class="btFbox w100" onclick="fnExcelDown()"><div class="btF">엑셀다운로드</div></div>';
					buffer+='</div>';
					var obj = jQuery.parseJSON(result.userFgMap);
					
					buffer+='<div style="display: none;" class="PrintArea">';
					buffer+='<table class="tb_list">';
					buffer+='<thead>';
					buffer+='<tr>';
					buffer+='<th><div>고객구분</div></th>';
					buffer+='<th><div>무역업번호</div></th>';
					buffer+='<th><div>사업자번호</div></th>';
					buffer+='<th><div>회사명</div></th>';
					buffer+='<th><div>휴폐업여부</div></th>';
					buffer+='<th><div>대표자명</div></th>';
					buffer+='<th><div>개인구분</div></th>';
					buffer+='<th><div>담당자</div></th>';
					buffer+='<th><div>전화번호</div></th>';
					buffer+='<th><div>담당자이메일</div></th>';
					buffer+='</tr>';
					buffer+='</thead>';
					buffer+='<tbody>';
					for (var i=0; i < jsondata.length; i++){
						buffer+='<tr>';
						buffer+='<td class="alC">';
						var breakLoop = false;
						for(key in obj) {
						    if(!breakLoop)
						        if(key == _cmmutils_json_null(jsondata[i].corpUserFg)){
						            buffer+=obj[key];
						            breakLoop = true;
						        }
						}
						buffer+='</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].memberId+'</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].compNo+'</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].compKorNm+'</td>';
						buffer+='<td>';
						if(_cmmutils_json_null(jsondata[i].corpStateCd)!='01')
							buffer+='휴업';
						else if(_cmmutils_json_null(jsondata[i].corpStateCd)!='02')
							buffer+='	폐업';
						buffer+='</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].compKorCeo+'</td>';
						buffer+='<td class="alC">';
						breakLoop = false;
						for(key in obj) {
						    if(!breakLoop)
						        if(key == _cmmutils_json_null(jsondata[i].userFg)){
						            buffer+=obj[key];
						            breakLoop = true;
						        }
						}
						buffer+='</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].contactNm)+'</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].contactTel)+'</td>';
						buffer+='<td class="alC">'+_cmmutils_json_null(jsondata[i].contactEmail)+'</td>';
						buffer+='</tr>';
		    		}
					buffer+='</tbody>';
					buffer+='</table>';
					buffer+='</div>';
					buffer+='<form name="excelDownloadForm" method="post" action="/jsp/customer/ajax/excelListDownLoad.jsp" target="_blank">';
					buffer+='<input type="hidden" name="html" />';
					buffer+='</form>';
					loading(false);
			    	$("div#_info5").show();
			    	$("div#_info1234").hide();
					$('div#_info5').html(buffer);
					//$('#listPage').html(result.paginationInfo);
			}, 'put', false);
		}
		else{
			alert("등록가능한 파일은 xls 만 가능합니다.");
		}
	}
}

//엑셀 - happy81
function fnExcelUpload1() {   
	var fileDir = $("input:file[name=excel]").val();
	if(fileDir.length > 0){
		loading(true, LOADING_TEXT_01);
		
		var type = fileDir.substring(fileDir.lastIndexOf(".") + 1).toLowerCase();
		if("xls" == type){
		    $('form[name="searchform"]').attr("action", "./ajax/excelList.jsp");
		    $('form[name="searchform"]').attr("enctype", "multipart/form-data");
		    $('form[name="searchform"]').ajaxSubmit(options);
		    
		    return false;
		}
		else{
			alert("등록가능한 파일은 xls 만 가능합니다.");
		}
	}
}


var options = {
		 target:        '#_info5',   // target element(s) to be updated with server response 
	     beforeSubmit:  showRequest,  // pre-submit callback 
	     success:       showResponse,  // post-submit callback 
	     type:          "post",       
	     dataTypy :     "text",
	     resetForm: false        // reset the form after successful submit 
	};

function showRequest(formData, jqForm, options) { 
    //var queryString = $.param(formData);
    //console.log($.param(formData));
    return true; 
} 

//post-submit callback 
function showResponse(responseText, statusText, xhr, $form)  { 
		loading(false);
    	$("div#_info5").show();
    	$("div#_info1234").hide();
    	//$('form[name="searchform"]').attr("enctype", "");
    	var pagerOptions = {
    			container: $("div.pager"),  
    			updateArrows: true,
    			page: 0,
    			size: 10
    	};
    	$("table#pageTable").tablesorter().tablesorterPager(pagerOptions);
} 

function fnTab(tab){
	if(tab == 5){
    	$("div#_info5").show();
    	$("div#_info1234").hide();
	}
	else{
    	$("div#_info5").hide();
    	$("div#_info1234").show();
	}
}

function fnDownloadLink(){
	//location.href='/jsp/BM/BM_download.jsp?fileName=BM001020_SAMPLE.xls&realName=&gu=E';
	location.href="<c:url value='/cmm/cmmn/downloadFile.do'/>?targetPathName=kita.crm.mktUploadExcel.path&fileName=BM001020_SAMPLE.xls&outFileName=BM001020_SAMPLE.xls";
}


function fnAddInfo(){
	
	 $("img[data-corp-seq]").hover(function(){
	
			var url = "/jsp/customer/boardOrgInfo.jsp";
			
			if($(this).next().length == 0){
				$(this).parent().append($("<div class='psTitle'></div>").hide().load(url+"?gubun=C&seq=" + $(this).attr("data-corp-seq"), function(){
					if($.trim($(this).text()) != "")	$(this).show();
				}));
			}else{
				if($.trim($(this).next().text()) != "")	$(this).next().show();
			}
				
		}, function(){
			$(this).next().hide();
		}
	);
	
}

function fnCorpDetail(corpSeq, memberId){
	var parameter = "&corpSeq=" + corpSeq + "&memberId=" + memberId;
	window.open("/jsp/customer/KCustBasicInfoDetail_company.jsp?popup=Y" + parameter,'detailCorp','status=yes,scrollbars=yes,resizable=yes,width=1000,height=540,left=50,top=50');
}
function fnPersonDetail(personSeq, memberId){
	var url = "/jsp/customer/KCustBasicInfoDetail_private.jsp?customerKind=P&popup=Y&callback=reloadStaff&personSeq=" + personSeq +"&memberId="+memberId;
	window.open(url,'newWin4','status=yes,scrollbars=yes,resizable=yes,width=1000,height=600,left=50,top=50');
}


/**
 * 통계보기
 */
function viewStatMenu(){
	if($("#customerCount").text() == 0) {	
		alert("고객 조회 후 통계보기 가능합니다.");
		return;
	}
	
	$("#viewSTATS").show();
}

function closeStatMenu(){
	$("#viewSTATS").hide();
}

/**
 * 통계화면으로 이동
 */
function fnStatistics(arg){
	var url ;
	var customerCount = $("#customerCount").text();
	$("form[name=maketForm] > input[name='customerCount']").val(customerCount);
	var sql = $("input[data-name='sql']").val();
	$("form[name=maketForm] > input[name='sql']").val(sql);
	
	//선호서비스통계
	if(arg == 1){
		url="/jsp/SR/stat_prefer_svc.jsp?S_MENU_CODE=7105";	
	}//재참가율통계
	else if(arg == 2){
		url="/jsp/SR/stat_reentrance_rate.jsp?S_MENU_CODE=7106";
	}/* //고객만족도통계
	else if(arg == 3){
		url="/jsp/SR/stat_customer_satisfaction.jsp?S_MENU_CODE=7109";
	} */
	
	loading(true, LOADING_TEXT_01);
 	$("form[name=maketForm]").attr("action", url).submit();
}
	
