/* 
 * 4번째 Tab 검색버튼 클릭시 호출 함수
 */
function search03() {
	
	//지부관할 값 셋팅	
	if( $("input[name=selectJibu3]").is(":checked") ){
		$("input[name=selectJibu3]").val("Y");
	}else{
		$("input[name=selectJibu3]").val("");
	}
	
	
	for(i=0; i<$("input:radio[name=newCustomer]").length; i++) {
		if($("input:radio[name=newCustomer]")[i].checked) {
			if($("input:radio[name=newCustomer]")[i].value == "1"){
				if(document.searchform.SDate.value == "") {
					alert("기간을 입력해 주세요.");
					document.searchform.SDate.focus();
					return;
				}
				if(document.searchform.EDate.value == "") {
					alert("기간을 입력해 주세요.");
					document.searchform.EDate.focus();
					return;
				}
			}
		}
		
	}
	
	loading(true, LOADING_TEXT_01);
	
	document.searchform.mode.value = "searchCustomer";
    document.searchform.customerSearchOptionKind.value = CUSTOMER_SEARCH_OPTION_KIND_IS_MEMBERINFO;
	document.searchform.isResult.value = "N";
	document.searchform.currentPage.value = 1;
	//document.searchform.target = "xframe";
	//document.searchform.action ="/jsp/customer/KCustBasicInfoSearch_company.jsp";	
	//document.searchform.submit();
	makeResultList();
}

/* 
 * 업종선택팝업을 호출한다.
 */
function openPopupJongmokSearch() {
	
	var jongmokReturn = window.showModalDialog("/kita/cstmrinfomanage/cstmrinqire/corpotationcustomersearchPsitnsclsrt.do?tbname=jongmok_cdode1", null, "dialogWidth:500px;dialogHeight:400px;status:no;");
	if(jongmokReturn == undefined || jongmokReturn == "undefined") jongmokReturn = window.returnValue;
	if(jongmokReturn != null) {
    	
		var jongmokReturnText = jongmokReturn.split('_/');
		document.searchform.jongmokCd.value    = jongmokReturnText[0];
		document.searchform.jongmokValue.value = jongmokReturnText[1];
    }
} 
	

/* 
 * 품목선택팝업을 호출한다.
 */
function openPopupItemList() {
	
	document.searchform.itemText.value  = "";
	document.searchform.itemValue.value = "";
			
	if(document.searchform.itemCode.value == "hs_cd") {
		var itemReturn = window.showModalDialog("/jsp/popup/KItemListIS.jsp?condPrdtbrGb=H&form_name=frm&class=item", null, "dialogWidth:450px;dialogHeight:300px;status:no;");
		
		if(itemReturn != null) {
			var itemReturnText = itemReturn.split('_/');
			
			document.searchform.itemText.value  = itemReturnText[0];
			document.searchform.itemValue.value = itemReturnText[1];
	    }
	}
}

/**
 * 해당 페이지로 이동한다.
 */
function jsGoPages(pageNo) {
    
	loading(true, LOADING_TEXT_01);
	
	document.searchform.mode.value = "searchCustomer";
    document.searchform.currentPage.value = pageNo;
    //document.searchform.target = "xframe";
	//document.searchform.submit();
	makeResultList();
}

/**
 * 선택한 고객들을 관심고객으로 등록한다.
 */
function setMyInterestCustomer() {
	
	if(parseInt(document.searchform.targetCustomerCount.value) == 0) {	
		alert("관심고객을 선택하여 주세요.");
		return;
	}
	
	for(i=0; i<$("input:checkbox[name=targetCustomer]").length; i++) {
		if($("input:checkbox[name=targetCustomer]")[i].checked) {
			if($("#interestYn" + i).text() == "Y") {
				alert("이미 등록된 고객이 1건 이상 선택되었습니다. 확인하여 주세요.");
				return;
			}
		}
	}
	
	/*
	loading(true, LOADING_TEXT_02);

    document.searchform.mode.value = "setMyInterestCustomer";
    document.searchform.checkedCustomerList.value = document.searchform.corpSeqList.value;
	document.searchform.target = "xframe";
	document.searchform.submit(); */
	
	var popup = "/kita/cstmrinfomanage/cstmrinqire/individualcustomersearchIntrstcstmrregist.do?customerKind=C&flag=1&checkedCustomerList=" + document.searchform.corpSeqList.value;
	window.open(popup, "newWin7", "width=450px, height=350px, left=200px, top=150px");
}

/**
 * 고객상세정보를 조회한다.
 */
function goDetail(corpSeq, memberId) {

	loading(true, LOADING_TEXT_01);
	
	document.searchform.target = "_top";
	document.searchform.action = "./KCustBasicInfoDetail_company.jsp?previousPage=KCustBasicInfoSearch_company&corpSeq=" + corpSeq + "&memberId=" + memberId;
	document.searchform.submit();
}

/**
 * 고객리스트 전체를 체크 또는 해제한다.
 */
function checkAll() {

	var isChecked = false;
	var count = parseInt(document.searchform.targetCustomerCount.value);
	
    for(i=0; i<$("input:checkbox[name=targetCustomer]").length; i++) {
        if($("input:checkbox[name=targetCustomer]")[i].checked) {
    		isChecked = true;
    		break;
        }
    }
    
    if(isChecked) {
    	for(i=0; i<$("input:checkbox[name=targetCustomer]").length; i++) {
    		$("input:checkbox[name=targetCustomer]")[i].checked = false;
    	}
    	count = 0;
    } else {
    	for(i=0; i<$("input:checkbox[name=targetCustomer]").length; i++) {
    		$("input:checkbox[name=targetCustomer]")[i].checked = true;
    	}
    	count = $("input:checkbox[name=targetCustomer]").length;
    }
    
    document.searchform.targetCustomerCount.value = count;
}

/**
 * 선택한 고객들의 조회키를 반환한다.
 */
function getCheckedCustomerList() {
	
	var corpSeqList = "";
	
	for(i=0; i<$("input:checkbox[name=targetCustomer]").length; i++) {
		if($("input:checkbox[name=targetCustomer]")[i].checked) {
        	if(corpSeqList == "") {
        		corpSeqList = $("input:checkbox[name=targetCustomer]")[i].value;
			} else {
				corpSeqList = corpSeqList + "," + $("input:checkbox[name=targetCustomer]")[i].value;
			}
        }
    }

	return corpSeqList;
}

/**
 * 체크된 고객리스트의 숫자를 셋팅한다.
 */
 
 function checkTargetCustomer(targetCustomer) {
		
	if(targetCustomer.checked) {
		valueArr.push(targetCustomer.value);
	} else {
			for(var i = 0; i<valueArr.length; i++){
				if(valueArr[i] == targetCustomer.value){
					valueArr.splice(i, 1);
				}
			}
	}
	var str = "";
	for(var i = 0; i<valueArr.length; i++){
		if(i == 0){
			str = valueArr[0];
		}
		else{
			str += ','+ valueArr[i];
		}
	}
	document.searchform.corpSeqList.value = str;
	
	var count = parseInt(document.searchform.targetCustomerCount.value);
	
	if(targetCustomer.checked) {
		count += 1;
	} else {
		count -= 1;
	}
		
	document.searchform.targetCustomerCount.value = count;
		
}

 function setCheck(){
	if(document.searchform.corpSeqList.value != ""){
		valueArr = document.searchform.corpSeqList.value.split(',');
		for(i=0; i<valueArr.length; i++){
			$("input[name=targetCustomer]").filter('input[value='+valueArr[i]+']').attr("checked", "checked");
		}
	}
}

/**
 * 페이지당 조회건수를 수정한다. 
 */
function changeLineCountPerPage(value) {
	
	setLineCountPerPage(value);
	
	document.searchform.lineCountPerPage.value = value;
	
	if(document.searchform.totalCount.value != 0) {
		loading(true, LOADING_TEXT_01);
	
		document.searchform.mode.value = "searchCustomer";
	    document.searchform.currentPage.value = 1;
	    //document.searchform.target = "xframe";
		//document.searchform.submit();
	    makeResultList();
	}
}

/**
 * 선택한 고객들을 마케팅한다.
 */
function executeMarketing() {
	if(parseInt($("#customerCount").text()) == 0) {	
		alert("고객 조회 후 마케팅실행 가능합니다.");
		return;
	}
	
	var checkedCustomerList = document.searchform.corpSeqList.value;
	
	var mktMake;
	
	mktMake = window.open('','mktMake2','status=no,scrollbars=yes,resizable=yes,width=640,height=650,left=50,top=50');	
	document.maketForm.corpSeq.value=checkedCustomerList;
	document.maketForm.customerSearchOptionKind.value = document.searchform.customerSearchOptionKind.value;
	document.maketForm.jongmokCd.value = document.searchform.jongmokCd.value;
	document.maketForm.jongmokValue.value = document.searchform.jongmokValue.value;
	document.maketForm.itemCode.value = document.searchform.itemCode.value;
	document.maketForm.itemValue.value = document.searchform.itemValue.value;
	document.maketForm.itemText.value = document.searchform.itemText.value;
    document.maketForm.optionMemberId.value = document.searchform.optionMemberId.value;
    document.maketForm.corpKorNm1.value = document.searchform.corpKorNm1.value;
    document.maketForm.ceoKorNm.value = document.searchform.ceoKorNm.value;
    document.maketForm.compNo.value = document.searchform.compNo.value;
    document.maketForm.firmNo.value = document.searchform.firmNo.value;
    
	document.maketForm.corpKorNm2.value = document.searchform.corpKorNm2.value;
	document.maketForm.yyyy.value = document.searchform.yyyy.value;
	document.maketForm.mm.value = document.searchform.mm.value;
	document.searchform.tradeAcmKind.value = $("input:radio[name=tradeAcmKind]:checked").val();
	document.maketForm.tradeAcmKind.value = document.searchform.tradeAcmKind.value;
	document.searchform.ieGbn.value = $("input:radio[name=ieGbn]:checked").val();
	document.maketForm.ieGbn.value = document.searchform.ieGbn.value;
	document.maketForm.condRelCd.value = document.searchform.condRelCd.value;
	document.maketForm.condRelNm.value = document.searchform.condRelNm.value;
	document.maketForm.condCtrCd.value = document.searchform.condCtrCd.value;
	document.maketForm.condCtrNm.value = document.searchform.condCtrNm.value;
	document.maketForm.tradeAmtFrom.value = document.searchform.tradeAmtFrom.value;
	document.maketForm.tradeAmtTo.value = document.searchform.tradeAmtTo.value;
	document.maketForm.sortFg.value = $("input:radio[name=sortFg]:checked").val();	
	document.maketForm.tab1AddressType.value = document.searchform.tab1AddressType.value;
	document.searchform.itemFg.value = $("input:radio[name=itemFg]:checked").val();
	document.maketForm.itemFg.value = document.searchform.itemFg.value;
	document.maketForm.condPrdtCd.value = document.searchform.condPrdtCd.value;
	document.maketForm.condPrdtNm.value = document.searchform.condPrdtNm.value;
	document.maketForm.condPrdtbrGb.value = document.searchform.condPrdtbrGb.value;
	document.maketForm.qtyUnit.value = document.searchform.qtyUnit.value;
	document.maketForm.sql.value = document.searchform.sql.value;

	var ArrTab1CityCode = new Array();
	for(i=0; i<$("input:checkbox[name=tab1CityCode]").length; i++) {
		if($("input:checkbox[name=tab1CityCode]")[i].checked == true){
			ArrTab1CityCode.push( $("input:checkbox[name=tab1CityCode]")[i].value);
		}
	}
	var str1 = "";
	for(var i = 0; i<ArrTab1CityCode.length; i++){
		if(i == 0){
			str1 = ArrTab1CityCode[0];
		}
		else{
			str1 += ','+ ArrTab1CityCode[i];
		}
	}
	document.searchform.tab1CityCodeHidden.value = str1;
	document.maketForm.tab1CityCodeHidden.value = document.searchform.tab1CityCodeHidden.value;
	
	var Arrtab1AddressType = new Array();
	for(i=0; i<$("input:checkbox[name='tab1AddressType']").length; i++) {
		if($("input:checkbox[name='tab1AddressType']")[i].checked == true){
			Arrtab1AddressType.push( $("input:checkbox[name='tab1AddressType']")[i].value);
		}
	}
	var str3 = "";
	for(var i = 0; i<Arrtab1AddressType.length; i++){
		if(i == 0){
			str3 = Arrtab1AddressType[0];
		}
		else{
			str3 += ','+ Arrtab1AddressType[i];
		}
	}
	document.searchform.tab1AddressTypeHidden.value = str3;
	document.maketForm.tab1AddressTypeHidden.value = document.searchform.tab1AddressTypeHidden.value;
	
    document.maketForm.corpKorNm3.value = document.searchform.corpKorNm3.value;
    document.maketForm.servicePID.value = document.searchform.servicePID.value;
    document.maketForm.serviceGID.value = document.searchform.serviceGID.value;
    document.maketForm.serviceSID.value = document.searchform.serviceSID.value;
    document.maketForm.serviceNm.value = document.searchform.serviceNm.value;
    document.maketForm.serviceSDate.value = document.searchform.serviceSDate.value;
    document.maketForm.serviceEDate.value = document.searchform.serviceEDate.value;
    document.maketForm.head.value = document.searchform.headHidden.value;
    document.maketForm.department.value = document.searchform.department.value;
                                    
    //tab4 조회조건
    document.maketForm.corpKorNm4.value = document.searchform.corpKorNm4.value;
    document.maketForm.userFg.value = document.searchform.userFg.value;
    document.searchform.feeYn.value = $("input:radio[name=feeYn]:checked").val();
    document.maketForm.feeYn.value = document.searchform.feeYn.value;
    document.maketForm.tab3AddressType.value = document.searchform.tab3AddressType.value;
    var Arr1 = new Array();
	for(i=0; i<$("input:checkbox[name=tab3CityCode]").length; i++) {
		if($("input:checkbox[name=tab3CityCode]")[i].checked == true){
			Arr1.push( $("input:checkbox[name=tab3CityCode]")[i].value);
		}
	}
	var str2 = "";
	for(var i = 0; i<Arr1.length; i++){
		if(i == 0){
			str2 = Arr1[0];
		}
		else{
			str2 += ','+ Arr1[i];
		}
	}
	document.searchform.tab3CityCodeHidden.value = str2;
    document.maketForm.tab3CityCodeHidden.value = document.searchform.tab3CityCodeHidden.value;
    document.searchform.newCustomer.value = $("input:radio[name=newCustomer]:checked").val();
    document.maketForm.newCustomer.value = document.searchform.newCustomer.value;
    document.maketForm.SDate.value = document.searchform.SDate.value;
    document.maketForm.EDate.value = document.searchform.EDate.value;
	document.maketForm.sUrl.value="KCustBasicInfoList_company";
	document.maketForm.target="mktMake2";
	document.maketForm.action="/jsp/SR/SR003010NewP.jsp";
	document.maketForm.submit();
	
} 

/**
 * 조회한 고객들을 엑셀로 저장한다.
 */
function downloadCustomer() {
	if(parseInt($("#customerCount").text()) == 0) {	
		alert("고객 조회 후 엑셀다운로드 가능합니다.");
		return;
	}
	
	//loading(true, LOADING_TEXT_01);

	document.searchform.mode.value = "downloadCustomer";
	//document.searchform.target = "xframe";
	//document.searchform.submit();
    commonUtil.ajaxCall('<c:url value="/kita/cstmrinfomanage/cstmrinqire/corpotationcustomersearchBassinfoForAjax.do" />', $("form#searchform").serialize(), function(result){
        location.href="<c:url value='/cmm/cmmn/downloadFile.do'/>?targetPathName="+result.reqDataVO.excelFilePath+"&fileName="+result.reqDataVO.excelFileName+"&outFileName="+result.reqDataVO.excelFileName;
    }, 'put', false);
}

