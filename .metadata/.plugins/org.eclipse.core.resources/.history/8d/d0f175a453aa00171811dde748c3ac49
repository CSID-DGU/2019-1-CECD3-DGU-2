/** 법인고객조회 - 기본정보 정보조건 */
var CUSTOMER_SEARCH_OPTION_KIND_IS_BUSINESSINFO      = "B";
var CUSTOMER_SEARCH_OPTION_KIND_IS_BUSINESSINFO_TEXT = "기본정보조회";
/** 법인고객조회 - 수출입실적 정보조건 */
var CUSTOMER_SEARCH_OPTION_KIND_IS_TRADEINFO         = "T";
var CUSTOMER_SEARCH_OPTION_KIND_IS_TRADEINFO_TEXT    = "수출입실적조회";
/** 법인고객조회 - 서비스이력 정보조건 */
var CUSTOMER_SEARCH_OPTION_KIND_IS_SERVICEINFO       = "S";
var CUSTOMER_SEARCH_OPTION_KIND_IS_SERVICEINFO_TEXT  = "서비스이력조회";
/** 법인고객조회 - 회원특성 정보조건 */
var CUSTOMER_SEARCH_OPTION_KIND_IS_MEMBERINFO        = "M";
var CUSTOMER_SEARCH_OPTION_KIND_IS_MEMBERINFO_TEXT   = "회원특성조회";

var CUSTOMER_SEARCH_OPTION_KIND_IS_EXCEL        = "E";
var CUSTOMER_SEARCH_OPTION_KIND_IS_EXCEL_TEXT   = "엑셀비교";
var valueArr = new Array();
/**
 * 화면을 초기화한다.
 */
$(document).ready(function() {
	
    var temp = $("#tabs").tabs();
	
	setCommonCodeBox_corp("131", "tab1AddressType");
     
    $("#tab3AddressType").html($("#tab1AddressType").html());
    
    $("input[name='userGubunBox1']").attr("checked", false);	
    $("input[name='userGubunBox2']").attr("checked", false);	
    
    loading(false);
	
 	/** E9에서 내용이 많은 Big 테이블을 Ajax로 불러들일대 Cell이 한 칸씩 밀리는 버그가 있다. **/
	if ( $.browser.msie ) {
	    var expr = new RegExp('>[ \t\r\n\v\f]*<', 'g');
	    $('#customerList').html( ($('#customerList').html() + "").replace(expr, '><')  );
	}
 
    document.searchform.lineCountPerPage.value = getLineCountPerPage();
    	
	$("#serviceSDate").setMask({ mask : '9999-99-99' });
	$("#serviceEDate").setMask({ mask : '9999-99-99' });
	/*
	 * hjh, 수출입실적-금액 검색조건이 3자리씩 끊기도록 변경
	$("#tradeAmtFrom").setMask({ mask : '99,999,999,999', type : 'reverse' });
	$("#tradeAmtTo"  ).setMask({ mask : '99,999,999,999', type : 'reverse' });
	*/
	$("#tradeAmtFrom").setMask({ mask : '999,999,999,999', type : 'reverse' });
	$("#tradeAmtTo"  ).setMask({ mask : '999,999,999,999', type : 'reverse' });
	$("#SDate").setMask({ mask : '99999999' });
	$("#EDate").setMask({ mask : '99999999' });
	
	if(document.searchform.pageFromDetail.value == "true") {	    

		loading(true, LOADING_TEXT_01);
	    
		if(document.searchform.customerSearchOptionKind.value == CUSTOMER_SEARCH_OPTION_KIND_IS_BUSINESSINFO) {
			$("#btn_goTab01").click();
		} else if(document.searchform.customerSearchOptionKind.value == CUSTOMER_SEARCH_OPTION_KIND_IS_TRADEINFO) {
			$("#btn_goTab02").click();
		} else if(document.searchform.customerSearchOptionKind.value == CUSTOMER_SEARCH_OPTION_KIND_IS_SERVICEINFO) {
			$("#btn_goTab03").click();
		} else if(document.searchform.customerSearchOptionKind.value == CUSTOMER_SEARCH_OPTION_KIND_IS_MEMBERINFO) {
			$("#btn_goTab04").click();
		} else if(document.searchform.customerSearchOptionKind.value == CUSTOMER_SEARCH_OPTION_KIND_IS_EXCEL) {
			$("#btn_goTab05").click();
		}
    	
    	initServiceCategoryCode("servicePID", "serviceGID", document.searchform.servicePIDHidden.value, document.searchform.serviceGIDHidden.value);
		initDepartmentCode("head", "department", document.searchform.headHidden.value, document.searchform.departmentHidden.value, "전체", "전체");
		
    	//document.searchform.userFg.value          = document.searchform.userFgHidden.value;
    	document.searchform.tab1AddressType.value = document.searchform.tab1AddressTypeHidden.value;
    	//document.searchform.tab3AddressType.value = document.searchform.tab3AddressTypeHidden.value;
    	document.searchform.boardOrgCd.value      = document.searchform.boardOrgCdHidden.value;
		    	
		//showOtherOption();

		document.searchform.target = "xframe";
		document.searchform.mode.value = "searchCustomer";
	    document.searchform.submit();
	
	} else {
		initServiceCategoryCode("servicePID", "serviceGID");
		initDepartmentCode("head", "department", "", "", "전체", "전체");
	}

});


	//기본정보 항목별 소팅
	function sortFildSearch01(columNm, sort){
		loading(true);
		document.searchform.mode.value = "searchCustomer";
	    document.searchform.customerSearchOptionKind.value = CUSTOMER_SEARCH_OPTION_KIND_IS_BUSINESSINFO;
		document.searchform.isResult.value = 'N';
		document.searchform.currentPage.value = 1;
	    document.searchform.columNm.value = columNm;
	    document.searchform.sort.value = sort;		
		//document.searchform.target = "xframe";
		//document.searchform.submit();
		loading(true);
		makeResultList();
	}
	
	function sortFildSearch01_1(){
		alert("sort...2");
	}
	
	//수출입 실적 항목별 소팅
	function sortFildSearch02(columNm, sort){
		loading(true, LOADING_TEXT_01);
		document.searchform.mode.value = "searchCustomer";
	    document.searchform.customerSearchOptionKind.value = CUSTOMER_SEARCH_OPTION_KIND_IS_TRADEINFO;
		document.searchform.isResult.value = false;
		document.searchform.currentPage.value = 1;
	    document.searchform.columNm.value = columNm;
	    document.searchform.sort.value = sort;		
		document.searchform.target = "xframe";
		document.searchform.submit();
	}

	function divChange(){
		if ( $.browser.msie ) {
			var expr = new RegExp('>[ \t\r\n\v\f]*<', 'g');
			$('#customerList').html( ($('#customerList').html() + "").replace(expr, '><')  );
		}
	}
	//서비스이력 항목별 소팅
	function sortFildSearch03(columNm, sort){
		loading(true, LOADING_TEXT_01);
		document.searchform.mode.value = "searchCustomer";
	    document.searchform.customerSearchOptionKind.value = CUSTOMER_SEARCH_OPTION_KIND_IS_SERVICEINFO;
		document.searchform.isResult.value = false;
		document.searchform.currentPage.value = 1;
	    document.searchform.columNm.value = columNm;
	    document.searchform.sort.value = sort;		
		document.searchform.target = "xframe";
		document.searchform.submit();
	}
	
	//회원특성별 항목별 소팅
