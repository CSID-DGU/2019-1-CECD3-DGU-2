@Service("loginService")
public class LoginServiceImpl extends EgovAbstractServiceImpl implements  LoginService {

    private static final Logger LOGGER = LoggerFactory.getLogger(LoginInfoServiceImpl.class);



    @Resource(name="loginDAO")
    private LoginDAO loginDAO;


    /**
     * <pre>
     * 1. 개요 : 로그인 정보 조회
     * </pre>
     * @Method Name : selectLoginInfo
     * @author : dgu
     * @date : 2017. 8. 14.
     * @see kr.ac.dgu.icip.login.service.LoginInfoService#selectLoginInfo(kr.ac.dgu.icip.cmm.vo.LoginInfoVO)
     * @param vo
     * @return
     * @throws Exception
     */
    public LoginVO selectLoginInfo(LoginVO vo) throws Exception {
        LoginVO loginvo = new LoginVO();
        try{
            loginvo = loginDAO.selectLoginInfo(vo);
        }catch(Exception ex){
            LOGGER.error("",ex);
        }
        return loginvo;
    }
    
